{{> header}}
<div id="myMovies">
    <h1>My Movies</h1>
    <div class="movies">
        {{#each data}}
            <div class="movie">
                <h2>Title: {{title}}</h2>
                <img src="{{imageUrl}}">
                <p>Available Tickets: {{tickets}}</p>
                <div class="btn-group">
                    <button><a href="#/object/update/{{objectId}}">Edit</a></button>
                    <button><a href="#/object/delete/{{objectId}}">Delete</a></button>
                    <button><a href="#/object/{{objectId}}/ticket">Buy Ticket</a></button>
                    <button><a href="#/object/details/{{objectId}}">Details</a></button>
                </div>
            </div>
        {{/each}}
    </div>
</div>

{{> footer}}